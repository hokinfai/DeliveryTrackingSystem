<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>RegistrationServlet</servlet-name>
		<display-name>RegistrationServlet</display-name>
		<description></description>
		<servlet-class>com.example.servlet.RegistrationServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<display-name>LoginServlet</display-name>
		<description></description>
		<servlet-class>com.example.servlet.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CompanyLoginServlet</servlet-name>
		<display-name>CompanyLoginServlet</display-name>
		<description></description>
		<servlet-class>com.example.servlet.CompanyLoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ChangeUserDetail</servlet-name>
		<display-name>ChangeUserDetail</display-name>
		<description></description>
		<servlet-class>com.example.servlet.ChangeUserDetail</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AddOrder</servlet-name>
		<display-name>AddOrder</display-name>
		<description></description>
		<servlet-class>com.example.servlet.AddOrder</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UpdateOrders</servlet-name>
		<display-name>UpdateOrders</display-name>
		<description></description>
		<servlet-class>com.example.servlet.UpdateOrders</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>OrderTracking</servlet-name>
		<display-name>OrderTracking</display-name>
		<description></description>
		<servlet-class>com.example.servlet.OrderTracking</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegistrationServlet</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CompanyLoginServlet</servlet-name>
		<url-pattern>/companyLogin</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ChangeUserDetail</servlet-name>
		<url-pattern>/modify</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddOrder</servlet-name>
		<url-pattern>/addOrder</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UpdateOrders</servlet-name>
		<url-pattern>/updateOrders</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>OrderTracking</servlet-name>
		<url-pattern>/OrderTracking</url-pattern>
	</servlet-mapping>
</web-app>
